            android:drawableTop="@drawable/position_btn2"
            android:text="右前卫"
            android:textColor="#ffffff" />

        <CheckBox
            android:id="@+id/rb_midfield5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="85dp"
            android:button="@null"
            android:drawableTop="@drawable/position_btn2"
            android:text="后腰"
            android:textColor="#ffffff" />

        <CheckBox
            android:id="@+id/rb_midfield1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="95dp"
            android:button="@null"
            android:drawableTop="@drawable/position_btn1"
            android:text="前腰"
            android:textColor="#ffffff" />

        <CheckBox
            android:id="@+id/rb_leftwing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="95dp"
            android:button="@null"
            android:drawableTop="@drawable/position_btn1"
            android:text="左边锋"
            android:textColor="#ffffff" />
        
        <CheckBox
            android:id="@+id/rb_striker1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="95dp"
            android:button="@null"
            android:drawableTop="@drawable/position_btn1"
            android:text="影子前锋"
            android:textColor="#ffffff" />

        <CheckBox
            android:id="@+id/rb_rightwing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="110dp"
            android:button="@null"
            android:drawableTop="@drawable/position_btn1"
            android:text="右边锋"
            android:textColor="#ffffff" />

        <CheckBox
            android:id="@+id/rb_striker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="125dp"
            android:button="@null"
            android:drawableTop="@drawable/position_btn1"
            android:text="前锋"
            android:textColor="#ffffff" />
        

