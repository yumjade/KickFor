<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:background="#f0f0f0"
	    android:orientation="vertical">
	    <RelativeLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:background="@drawable/title_style">
	        <ImageView
	            android:id="@+id/detail_back"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_alignParentLeft="true"
	            android:layout_centerVertical="true"
	            android:padding="15dip"
	            android:src="@drawable/back"
	            android:background="@drawable/select_unselected_3"/>"
	        <TextView 
		        android:id="@+id/detail_title"
		        android:layout_toRightOf="@id/detail_back"
		        android:layout_centerVertical="true"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:text="回顾详细"
		        android:textSize="15sp"
		        android:textColor="#ffffff"/>
	        <TextView
	            android:id="@+id/detail_edit"
	            android:layout_alignParentRight="true"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:text="编辑"
	            android:textSize="15sp"
	            android:textColor="#ffffff"
	            android:padding="5dip"
	            android:background="@drawable/rect_14"/>
	    </RelativeLayout>
	    <TextView
	        android:id="@+id/detail_date_place"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:background="#f5f5f5"
	        android:textSize="14sp"
	        android:textColor="#333333"
	        android:padding="10dip"
	        android:text="2013.8.9  海淀实验学校"/>
	    <RelativeLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:background="@drawable/rect_3"
	        android:padding="10dip">
	        <ImageView
		        android:id="@+id/iv_detail_my_team"
		        android:layout_width="60dip"
		        android:layout_height="50dip"
		        android:layout_alignParentLeft="true"
		        android:layout_alignParentTop="true"
		        android:layout_marginLeft="40dip"
		        android:layout_marginBottom="10dip"
		        android:src="@drawable/team_default"/>
	        <TextView 
		        android:id="@+id/detail_match_result"
		        android:layout_alignTop="@id/iv_detail_my_team"
		        android:layout_alignBottom="@id/iv_detail_my_team"
		        android:layout_centerHorizontal="true"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="10dp"
		        android:textSize="20sp"
		        android:gravity="center"
		        android:textColor="#333333"
		        android:text="3 - 2"/>
		    <TextView 
		        android:id="@+id/detail_match_type"
		        android:layout_below="@id/detail_match_result"
		        android:layout_centerHorizontal="true"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:textSize="12sp"
		        android:textColor="#6e6e6e"
		        android:text="友谊赛"/>
		    <ImageView
		        android:id="@+id/iv_detail_against_team"
		        android:layout_width="60dip"
		        android:layout_height="50dip"
		        android:layout_alignTop="@id/iv_detail_my_team"
		        android:layout_alignBottom="@id/detail_match_result"
		        android:layout_alignParentRight="true"
		        android:layout_marginRight="40dip"
		        android:src="@drawable/team_default"/>
		    <TextView
		        android:id="@+id/tv_detail_my_team_name"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@id/iv_detail_my_team"
		        android:layout_alignRight="@id/iv_detail_my_team"
		        android:layout_below="@id/iv_detail_my_team"
		        android:textSize="14sp"
		        android:gravity="center"
		        android:textColor="#333333"
		        android:text="亚平宁之师"/>
		    <TextView
		        android:id="@+id/tv_detail_against_team_name"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignLeft="@id/iv_detail_against_team"
		        android:layout_alignRight="@id/iv_detail_against_team"
		        android:layout_alignTop="@id/tv_detail_my_team_name"
		        android:textSize="14sp"
		        android:gravity="center"
		        android:textColor="#333333"
		        android:text="杨轩"/>
		    <TextView
		        android:id="@+id/detail_line1"
		        android:layout_width="match_parent"
		        android:layout_height="1dip"
		        android:layout_marginTop="10dip"
		        android:background="#dedede"
		        android:layout_below="@id/tv_detail_my_team_name"/>
		    <RelativeLayout
		        android:id="@+id/detail_r_goal"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_below="@id/detail_line1"
		        android:layout_marginTop="10dip"
		        android:paddingLeft="10dip"
		        android:paddingRight="10dip">
		        <TextView
		            android:id="@+id/goallogo"
		            android:layout_alignParentLeft="true"
		            android:layout_alignParentTop="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="15sp"
		            android:textColor="#6e6e6e"
		            android:text="进球："/>
		        <TextView
		            android:id="@+id/detail_who_goal"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:layout_alignTop="@id/goallogo"
		            android:layout_toRightOf="@id/goallogo"
		            android:layout_centerVertical="true"
		            android:textColor="#333333"
		            android:textSize="15sp"
		            android:text=""/>"
		    </RelativeLayout>
		    <RelativeLayout
		        android:id="@+id/detail_r_assist"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_below="@id/detail_r_goal"
		        android:paddingLeft="10dip"
		        android:layout_marginTop="5dip"
		        android:layout_marginBottom="5dip"
		        android:paddingRight="10dip">
		        <TextView
		            android:id="@+id/assistlogo"
		            android:layout_alignParentLeft="true"
		            android:layout_alignParentTop="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="助攻："
		            android:textColor="#6e6e6e"
		            android:textSize="15sp"/>
		        <TextView
		            android:id="@+id/detail_who_assist"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:layout_alignTop="@id/assistlogo"
		            android:layout_toRightOf="@id/assistlogo"
		            android:layout_centerVertical="true"
		            android:textColor="#333333"
		            android:textSize="15sp"
		            android:text=""/>
		    </RelativeLayout>
		    <RelativeLayout
		        android:id="@+id/detail_r_card"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_below="@id/detail_r_assist"
		        android:paddingLeft="10dip"
		        android:paddingRight="10dip">
		        <TextView
		            android:id="@+id/cardlogo"
		            android:layout_alignParentLeft="true"
		            android:layout_alignParentTop="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:textSize="15sp"
		            android:textColor="#6e6e6e"
		            android:text="红黄牌："/>
		        <TextView
		            android:id="@+id/detail_who_card"
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:layout_alignTop="@id/cardlogo"
		            android:layout_toRightOf="@id/cardlogo"
		            android:layout_centerVertical="true"
		            android:textColor="#333333"
		            android:textSize="15sp"
		            android:text=" "
		            android:drawableLeft="@drawable/yellow_card"/>"
		    </RelativeLayout>
	    </RelativeLayout>
	    <RelativeLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:background="@drawable/rect_3"
	        android:layout_marginTop="10dip"
	        android:paddingLeft="10dip"
	        android:paddingTop="10dip"
	        android:paddingBottom="10dip"
	        android:paddingRight="10dip">
	        <TextView
		        android:id="@+id/daochangduiyuan"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_alignParentTop="true"
		        android:textSize="15sp"
		        android:textColor="#333333"
		        android:drawableLeft="@drawable/green_card"
		        android:text="  到场队员"/>
	        <TextView
	            android:id="@+id/detail_line2"
	            android:layout_width="match_parent"
	            android:layout_height="1dip"
	            android:layout_marginTop="10dip"
	            android:background="#dedede"
	            android:layout_below="@id/daochangduiyuan"/>
		    <TextView
		        android:id="@+id/detail_who_come"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="10dip"
		        android:layout_below="@id/detail_line2"
		        android:gravity="left"
		        android:textSize="15sp"
		        android:textColor="#333333"
		        android:text=""/>
	    </RelativeLayout>
	    <!-- <RelativeLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="10dip"
	        android:background="@drawable/rect_3">
	        <TextView
		        android:id="@+id/suoyongzhenxing"
		        android:layout_alignParentTop="true"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:textSize="15sp"
		        android:textColor="#333333"
		        android:layout_marginTop="10dip"
		        android:layout_marginLeft="10dip"
		        android:drawableLeft="@drawable/green_card"
		        android:text="  所用阵型"/>
	        <TextView
	            android:id="@+id/detail_line3"
	            android:layout_width="match_parent"
	            android:layout_height="1dip"
	            android:layout_marginTop="10dip"
	            android:layout_marginLeft="10dip"
	            android:layout_marginRight="10dip"
	            android:background="#dedede"
	            android:layout_below="@id/suoyongzhenxing"/>
		    <TextView
		        android:id="@+id/detail_format"
		        android:paddingTop="10dip"
		        android:paddingBottom="5dip"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:text="4-1-4-1"
		        android:gravity="center_horizontal"
		        android:textSize="15sp"
		        android:textColor="#333333"
		        android:layout_below="@id/detail_line3"
		        android:drawableBottom="@drawable/court"/>
	    </RelativeLayout> -->   
	</LinearLayout>
</ScrollView>